  "scripts": {
    "build": "node entry.js auto:prod project-1",
    "build:dev": "node entry.js auto:prod project-1 dev",
    "prod": "webpack --mode production --env.NODE_ENV=production --progress",
    "prod.normal": "webpack --mode production --env.NODE_ENV=production.normal --progress",
    "dev": "webpack --mode development --env.NODE_ENV=development --progress",
    "start:dev": "webpack-dev-server --mode development --env.NODE_ENV=development --open",
    "start:prod": "webpack-dev-server --mode production --env.NODE_ENV=production --open",
    "start:prod.normal": "webpack-dev-server --mode production --env.NODE_ENV=production.normal --open",
    "auto:prod": "webpack --mode production --env.NODE_ENV=production --watch",
    "auto:prod.normal": "webpack --mode production --env.NODE_ENV=production.normal --watch",
    "auto:prod.dev": "webpack --mode development --env.NODE_ENV=development --watch"
  },
